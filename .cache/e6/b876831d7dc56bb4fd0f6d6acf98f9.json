n\n        if (_window) _window.remove()\n        _window = $('<div></div>')\n        _window.addClass('contextMenu_nekodev')\n\n        _window.html(`\n            <div class=\"content\">\n                <ul>\n                </ul>\n            </div>\n        `)\n\n        this.items.map(item => {\n            let li = $('<li></li>')\n            if (item.icon) {\n                li.html(item.icon + '&nbsp;&nbsp;&nbsp;' + item.label)\n            } else {\n                li.html(item.label)\n            }\n\n            li.on('click', evt => item.callback(evt, this, item))\n            _window.find('ul').first().append(li)\n        })\n\n        $('body').append(_window)\n\n        _window.css('left', `${x}px`)\n        _window.css('top', `${y}px`)\n    }\n\n}\n\n"},"lineCount":null}},"error":null,"hash":"41e2b149a880884bc2ff4c1a4d2b8939","cacheData":{"env":{}}}